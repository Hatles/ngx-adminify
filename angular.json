{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "app": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "demo/src",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/app",
            "index": "demo/src/index.html",
            "main": "demo/src/main.ts",
            "polyfills": "demo/src/polyfills.ts",
            "tsConfig": "demo/src/tsconfig.app.json",
            "assets": [
              "demo/src/favicon.ico",
              "demo/src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "demo/src/styles.css"
            ],
            "scripts": [],
                  "optimization": false,
                  "sourceMap": true,
                  "namedChunks": false,
                  "buildOptimizer": false
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "demo/src/environments/environment.ts",
                  "with": "demo/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "aot": true,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "app:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "app:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "app:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "demo/src/test.ts",
            "polyfills": "demo/src/polyfills.ts",
            "tsConfig": "demo/src/tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "demo/src/favicon.ico",
              "demo/src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "demo/src/styles.css"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "@ngx-adminify/router": {
      "root": "src/router",
      "sourceRoot": "src/router",
      "projectType": "library",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "src/router/tsconfig.lib.json",
            "project": "src/router/ng-package.json"
          },
          "configurations": {
            "production": {
              "project": "src/router/ng-package.prod.json",
              "tsConfig": "src/router/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/router/src/test.ts",
            "tsConfig": "src/router/tsconfig.spec.json",
            "karmaConfig": "src/router/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/router/tsconfig.lib.json",
              "src/router/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "@ngx-adminify/core": {
      "root": "src/core",
      "sourceRoot": "src/core",
      "projectType": "library",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "src/core/tsconfig.lib.json",
            "project": "src/core/ng-package.json"
          },
          "configurations": {
            "production": {
              "project": "src/core/ng-package.prod.json",
              "tsConfig": "src/core/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/core/src/test.ts",
            "tsConfig": "src/core/tsconfig.spec.json",
            "karmaConfig": "src/core/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/core/tsconfig.lib.json",
              "src/core/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "@ngx-adminify/entity": {
      "root": "src/entity",
      "sourceRoot": "src/entity/src",
      "projectType": "library",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "src/entity/tsconfig.lib.json",
            "project": "src/entity/ng-package.json"
          },
          "configurations": {
            "production": {
              "project": "src/entity/ng-package.prod.json",
              "tsConfig": "src/entity/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/entity/src/test.ts",
            "tsConfig": "src/entity/tsconfig.spec.json",
            "karmaConfig": "src/entity/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/entity/tsconfig.lib.json",
              "src/entity/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "packageManager": "npm",
    "analytics": false
  },
  "schematics": {
    "@schematics/angular:component": {
      "prefix": "demo",
      "style": "scss"
    },
    "@schematics/angular:directive": {
      "prefix": "demo"
    }
  }
}
